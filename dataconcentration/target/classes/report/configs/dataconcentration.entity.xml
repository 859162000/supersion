<?xml version="1.0" encoding="UTF-8"?>
<entityRoot>
	<!-- 根据主题控制报文生成按钮显示  key为主题，name为不显示的按钮的action,多action以逗号分隔-->
	<entityMap>
		<entityName>makeReportOptShowControl</entityName>
		<entity>
			<code>01</code>
			<name>生成本任务Excel文件@PostConfirm-report/ExportReportForTask,生成本任务IJ报文@PostConfirm-report/MakeIJFileAll,生成本机构Excel文件@PostConfirm-report/ExportReportForInst,生成本机构IJ报文@PostConfirm-report/MakeIJFileByOrg</name>
		</entity>
	</entityMap>
	
	<entityMap>
     <entityName>configParam</entityName>
       <entity>
       		<!-- false不包括，true包括,系统默认值为false -->
			<code>IsExcelExportPreData</code>
			<name>人行大集中报表导出Excel是否包括上期数据</name>
       </entity>
       
       <!-- ij文件生成时，无业务是否生成为0的数据,false不生成，true生成,系统默认值为false-->
		<entity>
			<code>IsMakeNoBusiness</code>
			<name>IJ文件生成时无业务是否生成为0的数据</name>
       </entity>
       
       <!-- ij文件生成时，指标值为0的记录是否统计,false不统计，true统计,系统默认值为false-->
       <entity>
			<code>IsZeroShow</code>
			<name>IJ文件生成时指标值为0的记录是否统计</name>
       </entity>     
   </entityMap>
</entityRoot>
