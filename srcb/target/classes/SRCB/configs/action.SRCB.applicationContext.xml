<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="filledWithMessageAction" class="SRCB.actions.imps.FilledWithMessageAction" scope="prototype">
       <property name="defaultServiceBeanId" value="filledWithMessageService"></property>
    </bean>
	<bean id="userInfoFilledWithMessageAction" class="SRCB.actions.imps.UserInfoFilledWithMessageAction" scope="prototype">
       <property name="defaultServiceBeanId" value="userInfoFilledWithMessageService"></property>
    </bean>
</beans>
