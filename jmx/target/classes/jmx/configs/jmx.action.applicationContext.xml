<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean id="jmxTaskAction" class="framework.actions.imps.BaseSAction" scope="prototype">
        <property name="defaultServiceBeanId" value="jmxTaskService"></property>
    </bean>
    <bean id="showCpuAction" class="framework.actions.imps.BaseSAction" scope="prototype">
        <property name="defaultServiceBeanId" value="showCpuService"></property>
    </bean>
    <bean id="showMemAction" class="framework.actions.imps.BaseSAction" scope="prototype">
        <property name="defaultServiceBeanId" value="showMemService"></property>
    </bean>
    <bean id="showJCAction" class="framework.actions.imps.BaseSAction" scope="prototype">
        <property name="defaultServiceBeanId" value="showJCService"></property>
    </bean>
    
</beans>
