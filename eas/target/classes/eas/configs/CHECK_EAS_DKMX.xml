<?xml version="1.0" encoding="UTF-8"?>
<reportCheckRoot>
	<!-- 校验名称：校验根据此名称做为校验实例-->
	<checkInstance name="CHECK_EAS_DKMX">	
		<!-- 参数值：配置的值为默认值，也可以通过系统传递进行替换-->		
		<paramValueList>
			<paramValue name="DTDATE" value="2013-12-09"></paramValue>
			<paramValue name="strInstCode" value=""></paramValue>
			<!--
		  <paramValue name="strTaskRptInstID" value="000000"></paramValue>
			<paramValue name="strFreq" value="000000"></paramValue> 
			-->
		</paramValueList>		
		
		<!-- 频度标志：可通过系统参数传递进行替换:1代表年报，2代表半年报，3代表季报，4代表月报，5代表日报-->
		<freqCode value="4"></freqCode>	
		<!-- 表名：真实数据库表的名称，一个校验实例中应具备唯一性-->
		<checkTable procedureName="CHECK_EAS_DKMX" procedureParam="DTDATE,strInstCode,rt" 
			originTable="EAS_DKMX" errorStateField="RPTCHECKTYPE" errorStateValue="3"  successStateValue="2"
			procedureDiscription="贷款明细"
			defaultLogicDaoBeanId="oracleProcedureResultSetDao">
			
			<!-- 参数字段：如果不配置此项为全表数据校验 
				type不填或则等于1代表字符，等于2代表时间-->
			<paramFieldList>
				<paramField paramName="DTDATE" field="DTDATE" type="2"></paramField>
				<paramField paramName="strInstCode" field="INSTINFO" type="1"></paramField>
			</paramFieldList>
			
			<!-- 唯一性字段组合：可理解为联合主键，用于未作说明的主外键关联-->
			<checkUniqueFieldList>
				<checkUniqueField name="AUTOID" discription="主键" originField="AUTOID"></checkUniqueField>
			</checkUniqueFieldList>
			
			<!-- 表内基础校验：被设置的字段每条数都需要校验 
				emptyCheck代表为空校验
				digitalCheck代表数字校验，dateCheck代表日期校验 
				upperCheck代表全大写字母校验，chnCheck代表汉字校验（可扩展其他特殊校验）
				startLength代表起始长度，endLength代表结束长度 
				valueDecimalLength的值代表小数位数的校验-->
				<checkFieldBasicList>
					<checkFieldBasic name="KHDM"  discription="客户代码" emptyCheck="true" endLength="500" ></checkFieldBasic>
					<checkFieldBasic name="KHMC"  discription="客户名称" emptyCheck="true" endLength="500" ></checkFieldBasic>
					<checkFieldBasic name="DKFFX"  discription="贷款发放行" emptyCheck="true" endLength="100" ></checkFieldBasic>
					<checkFieldBasic name="JRXKZH"  discription="金融许可证号" emptyCheck="true" startLength="15" endLength="15" ></checkFieldBasic>
					<checkFieldBasic name="XDYWZL"  discription="信贷业务种类" emptyCheck="true" endLength="100" ></checkFieldBasic>
          <checkFieldBasic name="DKLSHM"  discription="贷款流水号码" emptyCheck="true" endLength="30" ></checkFieldBasic>
          <checkFieldBasic name="DKJE"  discription="贷款金额" emptyCheck="true"  valueDecimalLength="2" endLength="20" ></checkFieldBasic>
          <checkFieldBasic name="WJFLFXZK"  discription="五级分类风险状况" emptyCheck="true" endLength="4" ></checkFieldBasic>
          <checkFieldBasic name="DBLX"  discription="担保类型" emptyCheck="true" endLength="30" ></checkFieldBasic>
          <checkFieldBasic name="DYDBRDM"  discription="第一担保人代码" emptyCheck="true" endLength="30" ></checkFieldBasic>
          <checkFieldBasic name="DYDBRMC"  discription="第一担保人名称" emptyCheck="true" endLength="200" ></checkFieldBasic>
          <checkFieldBasic name="DEDBRDM"  discription="第二担保人代码" emptyCheck="true" endLength="30" ></checkFieldBasic>
          <checkFieldBasic name="DEDBRMC"  discription="第二担保人名称" emptyCheck="true" endLength="200" ></checkFieldBasic>
          <checkFieldBasic name="DKTXXY"  discription="贷款投向行业" emptyCheck="true" endLength="60" ></checkFieldBasic>
          <checkFieldBasic name="SFGJXZXY"  discription="是否国家限制行业" emptyCheck="true" endLength="4" ></checkFieldBasic>
          <checkFieldBasic name="FFRQ"  discription="发放日期" ignoreVal="999991231" emptyCheck="true" dateCheck="true"  startLength="8" endLength="8" ></checkFieldBasic>
          <checkFieldBasic name="DQRQ"  discription="到期日期" ignoreVal="999991231" emptyCheck="true" dateCheck="true"  startLength="8" endLength="8" ></checkFieldBasic>
          <checkFieldBasic name="TJRQ"  discription="统计日期" ignoreVal="999991231" emptyCheck="true" dateCheck="true"  startLength="8" endLength="8" ></checkFieldBasic>
          <checkFieldBasic name="CJRQ"  discription="采集日期" ignoreVal="999991231" emptyCheck="true" dateCheck="true"  startLength="8" endLength="8" ></checkFieldBasic>
				                                                       
				</checkFieldBasicList>
				<checkFieldEffectiveList>                                      
         <checkFieldEffective name="WJFLFXZK" discription="五级分类风险状况" >
