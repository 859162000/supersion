<reportCheckRoot>
	<!-- 校验名称：校验根据此名称做为校验实例-->
	<checkInstance name="CHECK_EAS_DKZQ">	
		<!-- 参数值：配置的值为默认值，也可以通过系统传递进行替换-->		
		<paramValueList>
			<paramValue name="DTDATE" value="2014-08-31"></paramValue>
			<paramValue name="strInstCode" value=""></paramValue>
